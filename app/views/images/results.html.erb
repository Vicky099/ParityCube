<div class="row">
  <div class="col s12 m3">
    <h5>Selfie with friend contest</h5>
    <p>
      First Top three like images
    </p>
  </div>  
</div>
<% @top_like_images.each do |image| %>
  <div class="row">
    <div class="col s12 m3">
      <div class="card small">
        <div class="card-image waves-effect waves-block waves-light">
          <%= image_tag image.photo.url, :style => "width:302px;height:180px", :class => "activator" %>
        </div>
        <div class="card-content">
          <span class="card-title activator grey-text text-darken-4"><%= image.tag_line%><i class="material-icons right">more_vert</i></span>
          <p>
            Total Like: <%= image.like%>
          </p>
        </div>
        <div class="card-reveal">
          <span class="card-title grey-text text-darken-4">Image Owner Info<i class="material-icons right">close</i></span>
          <p>
            Name: <%=image.user.first_name%><%=image.user.last_name%>
          </p>
        </div>
      </div>
    </div>
  </div>
<%end%>
<hr/>
<p>
  First Top three favourite images
</p>   
<% @top_fav_images.each do |image| %>
  <div class="row">
    <div class="col s12 m3">
      <div class="card small">
        <div class="card-image waves-effect waves-block waves-light">
          <%= image_tag image.photo.url, :style => "width:302px;height:180px", :class => "activator" %>
        </div>
        <div class="card-content">
          <span class="card-title activator grey-text text-darken-4"><%= image.tag_line%><i class="material-icons right">more_vert</i></span>
          <p>
            Total Like: <%= image.favourite%>
          </p>
        </div>
        <div class="card-reveal">
          <span class="card-title grey-text text-darken-4">Image Owner Info<i class="material-icons right">close</i></span>
          <p>
            Name: <%=image.user.first_name%><%=image.user.last_name%>
          </p>
        </div>
      </div>
    </div>
  </div>
<%end%>   